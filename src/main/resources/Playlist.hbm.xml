<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="mll.beans.Playlist" table="Playlist">
        <id name="id" type="int">
            <column name="id" />
            <generator class="assigned" />
        </id>
       <property name="playlist_id" type="java.lang.Integer">
            <column name="playlist_id"/>
       </property>        
       <property name="song_id">
            <column name="song_id" length="200" />
        </property>        
    </class>
</hibernate-mapping>